<p id="notice"><%= notice %></p>

<h1>Carro Venta</h1>

<table>
  <thead>
    <tr>
      <th>Cantidad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @carro_venta.each do |carro_ventum| %>
      <tr>
        <td><%= carro_ventum.cantidad %></td>
        <td><%= link_to 'Mostar', carro_ventum %></td>
        <td><%= link_to 'Editar', edit_carro_ventum_path(carro_ventum) %></td>
        <td><%= link_to 'Eliminar', carro_ventum, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Carro de Venta', new_carro_ventum_path %>
