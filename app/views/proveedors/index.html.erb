<p id="notice"><%= notice %></p>

<h1>Proveedores</h1>


<div class="panel panel-default">
  
  <table class="table">
    
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Rut</th>
      <th>Correo</th>
      <th>Telefono</th>
      <th>Empresa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proveedors.each do |proveedor| %>
      <tr>
        <td><%= proveedor.nombre %></td>
        <td><%= proveedor.rut %></td>
        <td><%= proveedor.correo %></td>
        <td><%= proveedor.telefono %></td>
        <td><%= proveedor.empresa %></td>
        <td><%= link_to 'Mostrar', proveedor,class:"btn btn-success" %></td>
        <td><%= link_to 'Editar', edit_proveedor_path(proveedor),class:"btn btn-info"  %></td>
        <td><%= link_to 'Eliminar', proveedor, method: :delete, data: { confirm: 'Estas Seguro?' }, class:"btn btn-danger" %> </td>
      </tr>
    <% end %>
  </tbody>

  </table>
</div>



<br>

<%= link_to 'Nuevo Proveedor', new_proveedor_path, class:"btn btn-primary"  %>
<%= link_to 'Volver a Inicio', root_path %>
